init:
{
    # no initial settings
    dct_threads_before=2;
    dct_threads_after=3; 
    dvfs_freq_before=1200000;
    dvfs_freq_after=2000000;

}

default:
{
    # no default settings
    dct_threads_before=6;
    dct_threads_after=7; 
    dvfs_freq_before=2200000;
    dvfs_freq_after=2400000;
}

binary_0:
{
    # settings for this binary
    name = "test";
    # first test dct
    function_0:
    {
        name="test_dct";
        # change number of threads before entering the region
        dct_threads_before=4;
        # change number of threads after exiting the region
        dct_threads_after=5;
    };		
    
    # now we test files
    function_1:
    {
        name="test_file"
        # write something to this file whenever a region is entered
        file_0:
        {
            name="./test_file";
            before="1\n";
            after="0\n";
            # offset (int) would be an additional parameter that is not used here
        };

    };

    # we shoudn't forget the frequencies
    function_2:
    {
        name="test_dvfs";
        # change frequency before and after
        dvfs_freq_before=1300000; 
        dvfs_freq_after=2500000;

    };

    # and last but not least we test x86_adapt
    function_3:
    {
        name="test_x86a";
        # change settings from cc device driver
        #x86_adapt_AMD_Stride_Prefetch_before = 1;
        #x86_adapt_AMD_Stride_Prefetch_after = 0;

    };
};

